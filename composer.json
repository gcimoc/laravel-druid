{
    "name": "gcimoc/laravel-druid",
    "description": "Druid Package for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "israel-dominguez-garcia",
            "email": "israel.dominguez.garcia@gmail.com"
        }
    ],
    "version": "dev-master",
    "require": {
        "gcimoc/druid-php-sdk": "dev-master",
        "cache/apcu-adapter": "1.0.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gcimoc/druid-php-sdk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Genetsis\\Druid\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Genetsis\\Druid\\DruidServiceProvider"
            ]
        }
    }
}
